{
  "name": "ToDo - Mouse less",
  "description": "Manage your daily tasks more efficiently by using only your keyboard.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/yPerrot/TODO-mouse-less",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "ToDo - Mouse less",
    "default_icon": {
      "16": "./icons/logo16.png",
      "48": "./icons/logo48.png",
      "128": "./icons/logo128.png"
    }
  },
  "icons": {
    "16": "./icons/logo16.png",
    "48": "./icons/logo48.png",
    "128": "./icons/logo128.png"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+T"
      }
    }
  }
}
{
  "name": "TODO List Mouse Less",
  "description": "Manage your task more efficiantly by using only keybord.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/yperrot/todo-list-extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "TODO List",
    "default_icon": {
      "48": "logo48.png",
      "96": "logo96.png"
    }
  },
  "icons": {
    "48": "logo48.png",
    "96": "logo96.png"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}